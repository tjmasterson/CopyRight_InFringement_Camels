<h1><%=@survey.title%></h1>

<form action="/surveys/<%=@survey.id%>/complete" method="post"/>
<%@questions.each do |question|%>
  <%=question.content%><br>
    <%question.choices.each do |choice|%>
      <input type ="radio" name="question[<%=question.id%>]" value="<%=choice.content%>"><%=choice.content%></input><br>
    <%end%>
    <br><br>
<%end%>
<input type="submit" value="Complete Survey!"/>
<br><br>
</form>

